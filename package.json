{
  "name": "domodule",
  "version": "6.2.0",
  "description": "Class based modules",
  "main": "lib/domodule.js",
  "module": "dist/domodule.js",
  "keywords": [
    "module",
    "browser"
  ],
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "jest",
    "pretest": "scriptkit build",
    "prepublishOnly": "scriptkit"
  },
  "watch": {
    "lib/*.js": [
      "lint",
      "test"
    ],
    "test/*.js": [
      "lint",
      "test"
    ]
  },
  "repository": "https://github.com/firstandthird/domodule",
  "author": "First+Third",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-firstandthird": "^5.0.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "scriptkit": "^3.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": "firstandthird",
    "plugins": [
      "compat",
      "jest"
    ],
    "rules": {
      "compat/compat": 2
    }
  },
  "dependencies": {
    "attrobj": "^3.1.0",
    "aug": "^3.1.1",
    "domassist": "^2.2.0"
  },
  "scriptkit": {
    "files": {
      "dist/domodule.js": "lib/domodule.js"
    },
    "scripts": {
      "commonjs": {
        "ignore": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  }
}
